  <jfs:users
  	  rdf:resource="https://jazzjts.bt.bombardier.net:9443/jts/users"   />
  
  <!-- Service to redirect to the resource that represents the authenticated user -->
  <jfs:currentUser
  	  rdf:resource="https://jazzccm.bt.bombardier.net:9443/ccm/whoami"   />
  	  
